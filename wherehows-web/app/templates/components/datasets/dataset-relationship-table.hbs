{{#dataset-table fields=relationships as |relationTable|}}
  {{#relationTable.head as |head|}}
    {{#head.column}}Dataset{{/head.column}}
    {{#head.column}}All Types{{/head.column}}
    {{#head.column}}Actor{{/head.column}}
    {{#head.column}}Last Modified{{/head.column}}
  {{/relationTable.head}}

  {{#relationTable.body as |body|}}
    {{#each relationTable.data as |dataset|}}
      {{#body.row field=dataset as |row|}}
        {{#row.cell}}
          {{#link-to 'datasets.dataset' 'urn' (query-params urn=dataset.uri)}}
            <strong>
              {{dataset.nativeName}}
            </strong>
          {{/link-to}}
        {{/row.cell}}
        {{#row.cell}}
          {{titleize dataset.nativeType}}
        {{/row.cell}}
        {{#row.cell}}
          \{{Actor URN}}
        {{/row.cell}}
        {{#row.cell}}
          {{moment-calendar dataset.modifiedTime sameElse="MMM Do YYYY, h:mm a"}}
        {{/row.cell}}
      {{/body.row}}
    {{/each}}
  {{/relationTable.body}}
{{/dataset-table}}